<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="2300"
        motion:autoTransition="animateToEnd">

       <KeyFrameSet>
           <KeyAttribute
               motion:motionTarget="@+id/bigBubble"
               motion:framePosition="0"
               android:scaleX="1.2" />
           <KeyAttribute
               motion:motionTarget="@+id/bigBubble"
               motion:framePosition="0"
               android:scaleY="1.2" />
           <KeyAttribute
               motion:motionTarget="@+id/bigBubble"
               motion:framePosition="40"
               android:scaleX="15" />
           <KeyAttribute
               motion:motionTarget="@+id/bigBubble"
               motion:framePosition="40"
               android:scaleY="15" />
           <KeyAttribute
               motion:motionTarget="@+id/bigBubble"
               motion:framePosition="40"
               android:rotation="45" />
           <KeyAttribute
               motion:motionTarget="@id/bubble"
               motion:framePosition="0"
               android:scaleY="1.2" />
           <KeyAttribute
               motion:motionTarget="@+id/bigBubble"
               motion:framePosition="100"
               android:scaleX="50" />
           <KeyAttribute
               motion:motionTarget="@+id/bigBubble"
               motion:framePosition="0"
               android:scaleX="1.2" />
           <KeyAttribute
               motion:motionTarget="@+id/bigBubble"
               motion:framePosition="0"
               android:scaleX="1.2" />
           <KeyAttribute
               motion:motionTarget="@+id/bubble"
               motion:framePosition="0"
               android:scaleX="1.2" />
           <KeyAttribute
               motion:motionTarget="@+id/bubble"
               motion:framePosition="40"
               android:scaleX="15" />
           <KeyAttribute
               motion:motionTarget="@+id/bubble"
               motion:framePosition="40"
               android:scaleY="15" />
           <KeyAttribute
               motion:motionTarget="@+id/bubble"
               motion:framePosition="40"
               android:translationX="160dp" />
           <KeyAttribute
               motion:motionTarget="@+id/bigBubble"
               motion:framePosition="100"
               android:scaleY="50" />
           <KeyAttribute
               motion:motionTarget="@+id/bubble"
               motion:framePosition="100"
               android:scaleX="15" />
           <KeyAttribute
               motion:motionTarget="@+id/bubble"
               motion:framePosition="100"
               android:scaleY="15" />
           <KeyAttribute
               motion:motionTarget="@+id/partyHat"
               motion:framePosition="0"
               android:scaleX="1.2" />
           <KeyAttribute
               motion:motionTarget="@+id/partyHat"
               motion:framePosition="0"
               android:scaleY="1.2" />
           <KeyAttribute
               motion:motionTarget="@+id/partyHat"
               motion:framePosition="40"
               android:scaleX="8" />
           <KeyAttribute
               motion:motionTarget="@+id/partyHat"
               motion:framePosition="40"
               android:scaleY="8" />
           <KeyAttribute
               motion:motionTarget="@+id/partyHat"
               motion:framePosition="40"
               android:rotation="45" />
           <KeyAttribute
               motion:motionTarget="@+id/partyHat"
               motion:framePosition="100"
               android:scaleX="0" />
           <KeyAttribute
               motion:motionTarget="@+id/partyHat"
               motion:framePosition="100"
               android:scaleY="0" />
           <KeyAttribute
               motion:motionTarget="@+id/partyHat"
               motion:framePosition="100"
               android:rotation="-45" />
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>